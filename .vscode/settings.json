{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "search.exclude": {
      "**/node_modules": true,
      "**/bower_components": true,
  },
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.fixAll.stylelint": "explicit"
  }
}

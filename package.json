{
  "name": "cube-monorepo",
  "version": "0.0.0",
  "description": "Next generation NodeJS framework on top of H3",
  "packageManager": "pnpm@9.10.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "turbo": "turbo",
    "build": "turbo build --filter=!playground",
    "dev": "turbo dev --filter=!playground",
    "test": "turbo test --filter=!playground",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@eslint/js": "9.11.1",
    "eslint": "9.11.1",
    "globals": "15.9.0",
    "husky": "9.1.6",
    "turbo": "2.1.2",
    "typescript": "5.6.2",
    "typescript-eslint": "8.7.0",
    "unbuild": "3.0.0-rc.8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "unbuild"
      ]
    }
  }
}
